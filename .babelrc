{
  "presets": [
    "es2015",
    "es2016",
    "es2017"
  ],
  "plugins": [
    [
      "lodash-modularize",
      {
          "lodashVersion": "4.17.4"
      }
    ],
    "add-module-exports"
  ],
  "env": {
    "test": {
      "plugins": [
        [
          "istanbul",
          {
            "exclude": [
              "**/*.spec.js"
            ]
          }
        ],
        "rewire"
      ]
    },
  }
}